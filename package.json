{
  "name": "edegal",
  "version": "0.1.0",
  "description": "Fast web picture gallery",
  "main": "index.js",
  "scripts": {
    "prepublish": "bower install && gulp build",
    "start": "coffee server/server.coffee",
    "test": "mocha --reporter spec --compilers coffee:coffee-script/register spec/*_spec.coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/japsu/edegal"
  },
  "author": "Santtu Pajukanta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/japsu/edegal/issues"
  },
  "homepage": "https://github.com/japsu/edegal",
  "dependencies": {
    "bluebird": "^2.3.2",
    "coffee-script": "^1.8.0",
    "easyimage": "^1.0.2",
    "express": "^4.9.3",
    "fs-extra": "^0.11.1",
    "lodash": "^2.4.1",
    "mkdirp": "^0.5.0",
    "mongoose": "^3.8.16",
    "optimist": "^0.6.1",
    "winston": "^0.8.0"
  },
  "devDependencies": {
    "backbone": "^1.1.2",
    "backbone-relational": "^0.8.8",
    "bower": "^1.3.11",
    "browserify": "^5.12.0",
    "browserify-shim": "^3.7.0",
    "coffeeify": "^0.7.0",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^1.0.0",
    "gulp-jade": "^0.8.0",
    "gulp-minify-css": "^0.3.10",
    "gulp-rename": "^1.2.0",
    "gulp-streamify": "0.0.5",
    "gulp-stylus": "^1.3.3",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "mocha": "^1.21.4",
    "nib": "^1.0.3",
    "proxyquire": "^1.0.1",
    "should": "^4.0.4",
    "sinon": "^1.10.3",
    "vinyl-source-stream": "^1.0.0"
  },
  "browser": {
    "underscore": "lodash",
    "jquery": "./bower_components/jquery/dist/jquery.min.js",
    "transparency": "./bower_components/transparency/dist/transparency.min.js",
    "backbone": "./bower_components/backbone/backbone.js",
    "backbone-relational": "./bower_components/backbone-relational/backbone-relational.js",
    "hammerjs": "./bower_components/hammerjs/hammer.min.js",
    "jquery-hammerjs": "./bower_components/jquery-hammerjs/jquery.hammer.js"
  },
  "browserify-shim": {
    "jquery": "jQuery",
    "jquery-hammerjs": {
      "exports": "jQuery",
      "depends": [
        "jquery:$",
        "hammerjs:Hammer"
      ]
    },
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery:$",
        "lodash:_"
      ]
    },
    "backbone-relational": {
      "exports": "Backbone",
      "depends": [
        "backbone:Backbone"
      ]
    },
    "transparency": {
      "exports": "Transparency",
      "depends": [
        "jquery:$",
        "lodash:_"
      ]
    }
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim"
    ]
  }
}
