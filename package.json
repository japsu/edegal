  },
  "browserify-shim": {
    "jquery": "jQuery",
    "jquery-hammerjs": {
      "exports": "jQuery",
      "depends": [
        "jquery:$",
        "hammerjs:Hammer"
      ]
    },
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery:$",
        "lodash:_"
      ]
    },
    "backbone-relational": {
      "exports": "Backbone",
      "depends": [
        "backbone:Backbone"
      ]
    },
    "transparency": {
      "exports": "Transparency",
      "depends": [
        "jquery:$",
        "lodash:_"
      ]
    }
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim"
    ]
  }
}
